<%- include('./partials/head') %> 

<body class="bodyProducts">
	<!-- Header -->
	<%- include('./partials/header.ejs') %>
		<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Editar mi perfil</h2>
			</div>
			
			<section class="form-login">
				<article class="formulario">
					<form action="" method="GET" >
						<div class="form-group">
							<label class="form-texto-arriba">Nuevo correo electrónico</label>
							<input type="email" class="form-control" id="exampleInputEmail1"
								aria-describedby="emailHelp" placeholder="Example@email.com">
		
						</div>
						<div class="form-group">
							<label class="form-texto-arriba">Nuevo nombre de Usuario</label>
							<input type="text" class="form-control" id="exampleInputPassword1"
								placeholder="Creá tu nombre de usuario">
						</div>
						<div class="form-group">
							<label class="form-texto-arriba">Cambiá tu contraseña</label>
							<input type="text" class="form-control" id="exampleInputPassword1"
								placeholder="*********">
						</div>
						<div class="form-group">
							<label class="form-texto-arriba">Fecha de nacimiento</label>
							<input type="date" class="form-control" id="exampleInputPassword1"
								placeholder="">
						</div>
						<div class="form-group">
							<label class="form-texto-arriba">Nro. de documento</label>
							<input type="number" class="form-control" id="exampleInputPassword1"
								placeholder="99.999.999">
						</div>
						<div class="form-group">
							<label class="form-texto-arriba">Cambiá tu foto de perfil</label>
							<input type="file" class="form-control" id="exampleInputPassword1"
								placeholder="">
						</div>
						
						<button type="submit" class="btn-primary"><a href="/profile">Editar</a></button>
					</form>
				</article>
				<article class="foto-login">
					<img class="foto-lily" src="" alt="">
				</article>

			</section>

			


<!-- footer -->
<%- include('./partials/footer.ejs') %>
<!-- /footer -->

</body>

</html>